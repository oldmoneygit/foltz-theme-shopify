<style>
.plazos-page {
  background: #000;
  min-height: 100vh;
  padding: 3rem 0 4rem;
  position: relative;
  overflow: hidden;
}

.plazos-page::before {
  content: '';
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 1200px;
  height: 1200px;
  background: radial-gradient(circle, rgba(218, 241, 13, 0.03) 0%, transparent 70%);
  pointer-events: none;
}

.plazos-page__container {
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 1rem;
  position: relative;
  z-index: 1;
}

/* Header */
.plazos-page__header {
  text-align: center;
  margin-bottom: 3rem;
}

.plazos-page__header-icon-wrapper {
  width: 80px;
  height: 80px;
  margin: 0 auto 1.5rem;
  background: linear-gradient(135deg, rgba(218, 241, 13, 0.2), rgba(218, 241, 13, 0.1));
  border: 2px solid rgba(218, 241, 13, 0.3);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  animation: pulse 2s ease-in-out infinite;
}

@keyframes pulse {
  0%, 100% {
    transform: scale(1);
    box-shadow: 0 0 0 0 rgba(218, 241, 13, 0.4);
  }
  50% {
    transform: scale(1.05);
    box-shadow: 0 0 20px 10px rgba(218, 241, 13, 0);
  }
}

.plazos-page__header-icon {
  color: #DAF10D;
}

.plazos-page__title {
  font-size: 2rem;
  font-weight: 900;
  color: #fff;
  margin: 0 0 0.75rem 0;
  text-transform: uppercase;
  letter-spacing: 0.05em;
}

@media (min-width: 768px) {
  .plazos-page__title {
    font-size: 2.5rem;
  }
}

.plazos-page__subtitle {
  font-size: 1rem;
  color: rgba(255, 255, 255, 0.7);
  margin: 0;
}

@media (min-width: 768px) {
  .plazos-page__subtitle {
